@prefix ex: <http://example.org/> .


# Definición de la sala de clases ClassRoom03
ex:ClassRoom03
    ex:contains ex:Sensor347, ex:Table322 .

# Definición del sensor Sensor347
ex:Sensor347
    ex:hasMeasurement ex:Measurement8432 .

# Definición de la medición Measurement8432
ex:Measurement8432
    ex:hasTemperature 29 ;  # Temperatura
    ex:atTime "2022-09-12T12:01:12"^^xsd:dateTime .  # Tiempo de la medición

# Definición de la mesa Table322
ex:Table322
    ex:hasOwner ex:ETSIInformaticos .

# Definición de ETSIInformaticos
ex:ETSIInformaticos
    ex:hasName "ETSI Informáticos" .
